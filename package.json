{
  "name": "todo-list",
  "version": "1.0.0",
  "description": "To-do list",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch start-jsonserver",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "start-jsonserver": "json-server --watch src/api/db.json --port 3001"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pkhafizov/Todo-List.git"
  },
  "author": "Pavel Khafizov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pkhafizov/Todo-List/issues"
  },
  "homepage": "https://github.com/pkhafizov/Todo-List#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.3",
    "install": "^0.12.1",
    "json-server": "^0.14.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.3",
    "npm": "^6.4.1",
    "npm-run-all": "^4.1.5",
    "open": "6.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.3.1",
    "history": "^4.7.2",
    "jquery": "^3.4.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-datepicker": "^1.6.0",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  }
}
